{
    "name": "Youtube Results Counter",
    "version": "1.0.0",
    "description": "Counts search results on youtube",
    "manifest_version": 3,
    "author": "Sampurna Chapagain",
    "content_scripts": [
        {
          "matches": ["https://*.youtube.com/results*"],
          "js": ["script.js"]
        }
      ]
}
